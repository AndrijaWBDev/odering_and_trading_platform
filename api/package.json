{
  "name": "digishares-api",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_PATH=./src ts-node -r dotenv/config --files src/app.ts",
    "server": "NODE_PATH=./src ts-node-dev -r dotenv/config --files --respawn src/app.ts",
    "dbschema": "npx mysql-schema-ts mysql://$DB_USER:$DB_PASSWORD@$DB_SERVER:3306/$DB_Database > src/DBSchema.ts && prettier -w 'src/DBSchema.ts'",
    "test": "NODE_PATH=./src env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' NODE_ENV=testing ./node_modules/.bin/mocha -r dotenv/config --config mocharc.json",
    "test-file": "NODE_PATH=./src env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' NODE_ENV=testing ./node_modules/.bin/mocha -r dotenv/config --config mocharc-nospec.json",
    "test-integratoin-file": "NODE_PATH=./src env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' NODE_ENV=testing ./node_modules/.bin/mocha -r dotenv/config --config mocharc-integration-nospec.json",
    "migration:up": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "migration:down": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create -n"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.8.1",
    "@graphql-tools/schema": "^8.3.11",
    "@types/node-schedule": "^2.1.0",
    "apollo-server": "^3.7.0",
    "apollo-server-core": "^3.7.0",
    "apollo-server-express": "^3.3.0",
    "axios": "^0.24.0",
    "body-parser": "1.19.0",
    "bson": "^4.6.2",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "docusign-esign": "^5.15.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "got": "^11.8.2",
    "graphql": "^15.5.2",
    "graphql-depth-limit": "^1.1.0",
    "graphql-redis-subscriptions": "^2.5.0",
    "graphql-scalars": "^1.10.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^8.1.0",
    "graphql-upload": "^12.0.0",
    "graphql-ws": "^5.8.2",
    "hellosign-sdk": "^2.0.0",
    "i18n-iso-countries": "^7.4.0",
    "joi": "^17.4.0",
    "jws": "^4.0.0",
    "jwt-decode": "^3.1.2",
    "mathjs": "^10.1.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.25",
    "multer": "^1.4.4",
    "mysql2": "^2.2.5",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.7.5",
    "ravencoin-zmq-decoder": "^1.0.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.37",
    "uuid": "^8.3.2",
    "web3": "^1.7.0",
    "ws": "^8.5.3"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/docusign-esign": "^5.6.2",
    "@types/express-jwt": "^6.0.2",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/graphql-upload": "^8.0.7",
    "@types/hellosign-sdk": "^1.6.7",
    "@types/jws": "^3.2.4",
    "@types/mocha": "^9.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.7.7",
    "@types/nodemailer": "^6.4.0",
    "@types/request": "^2.48.7",
    "@types/sinon": "^10.0.11",
    "@types/uuid": "^8.3.1",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "jest": "^27.1.0",
    "lint-staged": "^12.1.7",
    "mocha": "^9.1.3",
    "mysqldump": "^3.2.0",
    "prettier": "^2.3.2",
    "sinon": "^13.0.1",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.2",
    "ws": "^8.6.0"
  },
  "lint-staged": {
    "src/**/*.{js,json,md,ts}": [
      "prettier --write"
    ],
    "src/**/*.{js,ts}": [
      "eslint --fix",
      "eslint --no-ignore --max-warnings=0"
    ]
  },
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "author": "",
  "license": "ISC"
}
