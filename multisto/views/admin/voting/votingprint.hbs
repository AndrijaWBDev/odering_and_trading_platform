        <br />


        <div class="row">
            <div class="col-md-1"> </div>
            <div class="col-md-10">
                <div class="card">
                    <div class="header">
                        <h4 class="title">
                            <img src="/img/meeting.png" width="30px;" style="padding-bottom:-10px;" /> &nbsp; {{MeetingRecord.title}}
                        </h4>
                    </div>

                    <div class="content">

                        <div class="row">
                            <div class="col-md-12">
                                <div class="row">
                                    <div class="col-md-12">
                                        {{MeetingRecord.contents}}
                                    </div>
                                    <br /><br /><br /><br />&nbsp;
                                    <div class="col-md-12">

                                            <div class="row">
                                                <div class="col-md-3">
                                                    <label>Place</label>
                                                </div>
                                                <div class="col-md-9">
                                                    <img src="/img/place.png" width="20px;" /> &nbsp;
                                                    {{MeetingRecord.place}}
                                                </div>
                                            </div>
                                            <br />

                                            <div class="row">
                                                <div class="col-md-3">
                                                    <label>Time</label>
                                                </div>
                                                <div class="col-md-9">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <img src="/img/date.png" width="20px;" /> &nbsp;
                                                            {{MeetingRecord.openonlydate}}
                                                        </div>
                                                        <div class="col-md-6">
                                                            <img src="/img/time.png" width="20px;" /> &nbsp;
                                                            {{MeetingRecord.starttime}}
                                                            &nbsp;&nbsp;to&nbsp;&nbsp;
                                                            {{MeetingRecord.closetime}}
                                                            &nbsp;&nbsp;
                                                            <span style="cursor:pointer" class="badge badge-light" title="{{MeetingRecord.timezonetitle}}">{{MeetingRecord.timezone}}</span>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <br />

                                            <div class="row">
                                                <div class="col-md-3">
                                                    <label>Responsible Person</label>
                                                </div>
                                                <div class="col-md-9">
                                                    <img src="/img/user.png" width="20px;" /> &nbsp;
                                                    {{MeetingRecord.nameResponsiblePerson}}
                                                    &nbsp;&nbsp;&nbsp;&nbsp;
                                                    {{MeetingRecord.phoneResponsiblePerson}}
                                                    &nbsp;&nbsp;&nbsp;&nbsp;
                                                    {{MeetingRecord.emailResponsiblePerson}}
                                                </div>
                                            </div>
                                            <br />

                                            <div class="row">
                                                <div class="col-md-3">
                                                    <label>Proxy Person</label>
                                                </div>
                                                <div class="col-md-9">
                                                    <img src="/img/user.png" width="20px;" /> &nbsp;
                                                    {{MeetingRecord.nameProxyPerson}}
                                                    &nbsp;&nbsp;&nbsp;&nbsp;
                                                    {{MeetingRecord.phoneProxyPerson}}
                                                    &nbsp;&nbsp;&nbsp;&nbsp;
                                                    {{MeetingRecord.emailProxyPerson}}
                                                </div>
                                            </div>
                                            <br />

                                    </div>
                                </div>
                            </div>
                        </div>
                        <br /> &nbsp;
                    </div>
                </div>
            </div>
        </div>

        <div class="row topPaddingBetweenPageSections">
            <div class="col-md-1"> </div>
            <div class="col-md-10">
                <div class="card">
                    <div class="header">
                        <h4 class="title">
                            <img src="/img/agendaitem.png" width="25px;" /> &nbsp;  Meeting Agenda
                        </h4>
                    </div>

                    <div class="content">

                        {{#each AgendaItems}}
                                <div class="row">
                                    <div class="col-md-7">
                                        <img src="/img/greendot.png" width="18px">
                                        &nbsp;
                                        <b>{{this.title}}</b>
                                        <br /><br />
                                        {{this.desc}}
                                        <br /><br />
                                        {{#if this.documents.length}}
                                            {{#each this.documents}}
                                                <div class="row">
                                                    <div class="col-md-10">
                                                        <img src="/img/document2.png" width="30px;" /> &nbsp;
                                                        {{this.title}}
                                                        <br />
                                                        <label>{{this.desc}}</label>
                                                    </div>
                                                </div>
                                            {{/each}}
                                        {{/if}}
                                    </div>
                                    <div class="col-md-1"></div>
                                    <div class="col-md-4">
                                        <span style="font-size:26px;">
                                            <img src="/img/box.png" width="26px"> &nbsp;
                                            Yes
                                        </span>
                                        <br /><br />
                                        <span style="font-size:26px;">
                                            <img src="/img/box.png" width="26px"> &nbsp;
                                            No
                                        </span>
                                        <br /><br />
                                        <span style="font-size:26px;">
                                            <img src="/img/box.png" width="26px"> &nbsp;
                                            Abstention
                                        </span>
                                    </div>
                                </div>
                                <br /><br />
                                <hr />
                            {{/each}}

                        <br /> &nbsp;
                    </div>
                </div>
            </div>
        </div>



