<hr /><div class="row">    <div class="col-md-4">        <h5>Contract Documents</h5>    </div>    <div class="col-md-2">        {{#ifIn 33 Data.AdminRights}}            <button type="button" onclick="return newDoc()" class="btn btn-info btn-fill btn-sm"> &nbsp; New Contract &nbsp; </button>        {{/ifIn}}    </div></div>{{#if filesRec.length}}    {{#each filesRec}}        <div class="row"  style="padding-top:10px;">            <div class="col-md-8" >                {{#ifIn 33 ../Data.AdminRights}}                    {{#unless this.offerID}}                        <a onclick="deletedoc({{this.ID}}, {{this.directoryid}})" class="linkStyle100 hovercolor" ><img src="/img/cross.png" width="13px;" /></a> &nbsp;                    {{else}}                    {{/unless}}                {{/ifIn}}              {{#if this.enabled}}                <img src="/img/greendot.png" width="13px;"  />              {{else}}                <img src="/img/reddot.png" width="13px;"  />              {{/if}}                <img src="/img/document.png" width="25px;"  />                <a class="linkStyle100"  href="doc?id={{this.ID}}&dirid={{this.directoryid}}"> {{this.title}} </a>            </div>            <div class="col-md-4">                {{#if this.offerID}}                    &nbsp;                    <a style="cursor: pointer;" onclick="gotoOfferedContractList({{this.ID}}, {{this.offerID}}, {{this.directoryid}})"><img src="/img/signed.png" width="22px;"  /></a>                    &nbsp;&nbsp;                    {{#if this.newContracts}}                        <span style="font-size:12px; color:red;">{{this.newContracts}} New </span> &nbsp;&nbsp;                    {{/if}}                    <span style="font-size:12px;"> {{this.signedContracts}} Contract </span>                {{else}}                    <a onclick="editdoc({{this.ID}}, {{this.directoryid}})" class="linkStyle100 hovercolor" ><img src="/img/edit.png" width="25px;" data-toggle="tooltip" data-placement="top" title="Edit/View Document" /></a>                    {{#ifIn 17 ../Data.AdminRights}}                        {{#if this.isactiveforinvestors}}                            <a onclick="showMessageCannotOffer()" class="linkStyle100 hovercolor" ><img src="/img/offer.png" width="32px;"/></a>                        {{else}}                            <a onclick="senddoc({{this.ID}})" class="linkStyle100 hovercolor" ><img src="/img/offer.png" width="32px;"  data-toggle="tooltip" data-placement="top" title="Offer Document as Contract to Investors"/></a>                        {{/if}}                    {{/ifIn}}                    {{#ifIn 33 ../Data.AdminRights}}                        <a onclick="activatedocumentforinvestor({{this.ID}}, {{this.directoryid}},                            {{#if this.isactiveforinvestors}} 0 {{else}} 1 {{/if}}, 0)" class="linkStyle100 hovercolor"  data-toggle="tooltip" data-placement="top"                            {{#if this.isactiveforinvestors}}                           title="Document is currently active for Investor review and suggestions. Click to deactivate document for investor review"                            {{else}}                           title="Click to activate document for Investor review and take suggestions"                            {{/if}}                        >                            {{#if this.isactiveforinvestors}}                                <img src="/img/reddot.png" width="23px;" />                            {{else}}                                <img src="/img/emptycircle.png" width="23px;" />                            {{/if}}                        </a>                    {{/ifIn}}                {{/if}}            </div>        </div>    {{/each}}{{else}}    <label>No Draft Contracts Found</label>{{/if}}